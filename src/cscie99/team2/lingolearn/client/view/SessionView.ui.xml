<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:mail='urn:import:cscie99.team2.lingolearn.client.view'>

  <g:HTMLPanel>

    <g:FlowPanel ui:field='sessionHeader'></g:FlowPanel>

    <g:FlowPanel ui:field='cardArea'></g:FlowPanel>

    <g:FlowPanel ui:field='knowledgeAssessmentArea' styleName='btn-group'>
        <g:Button ui:field='knowledgeLow' styleName='btn btn-default btn-lg'>I had no clue</g:Button>
        <g:Button ui:field='knowledgeMedium' styleName='btn btn-default btn-lg'>I sorta knew it</g:Button>
        <g:Button ui:field='knowledgeHigh' styleName='btn btn-default btn-lg'>I definitely knew it</g:Button>
    </g:FlowPanel>

  </g:HTMLPanel>

</ui:UiBinder>
